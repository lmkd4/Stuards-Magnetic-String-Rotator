#include <Servo.h>

int Pushbutton = 0;

Servo servo_6;

void setup()
{
  pinMode(2, OUTPUT);
  pinMode(9, OUTPUT);
  pinMode(3, OUTPUT);
  pinMode(4, OUTPUT);
  pinMode(5, OUTPUT);
  pinMode(7, INPUT);
  Serial.begin(9600);
  servo_6.attach(6, 500, 2500);
}

